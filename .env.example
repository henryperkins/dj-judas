# Facebook Integration (optional)
VITE_FACEBOOK_APP_ID=your_facebook_app_id
VITE_FACEBOOK_PIXEL_ID=your_facebook_pixel_id

# Instagram oEmbed (server-side)
# Generate via Facebook App: {app-id}|{client-token} or a valid page access token
IG_OEMBED_TOKEN=your_facebook_app_access_token

# Spotify OAuth (server-side)
SPOTIFY_CLIENT_ID=your_spotify_client_id

# Spotify Artist ID - DJ Lee & Voices of Judah
VITE_SPOTIFY_ARTIST_ID=5WICYLl8MXvOY2x3mkoSqK

# Social Media Links
VITE_INSTAGRAM_HANDLE=iam_djlee
VITE_FACEBOOK_PAGE=MidWestScreamers
VITE_SOUNDCLOUD_URL=https://soundcloud.com/dj-lee-voices-of-judah

# Optional: Universal social embed default URL
# Supports TikTok, YouTube, X/Twitter, LinkedIn, Pinterest, Facebook, Instagram post URLs
# Example:
# VITE_SOCIAL_EMBED_URL=https://www.youtube.com/watch?v=ysz5S6PUM-U

# Contact Email
VITE_CONTACT_EMAIL=V.O.J@icloud.com

# Medusa Storefront (optional for catalog/cart)
VITE_MEDUSA_URL=https://api.your-medusa.com
VITE_MEDUSA_PUBLISHABLE_KEY=pk_your_publishable_key

# Medusa Admin (server-side; used by Worker to proxy admin API)
# This should be the same base URL as VITE_MEDUSA_URL but available to the Worker runtime.
MEDUSA_URL=https://api.your-medusa.com

# Cloudflare Images (optional for admin uploads)
# Create a token with Images:Edit permission
CF_IMAGES_ACCOUNT_ID=your_account_id
CF_IMAGES_API_TOKEN=your_images_api_token
# Optional: name of delivery variant to use, e.g., 'public'
CF_IMAGES_VARIANT=public
VITE_CF_IMAGES_VARIANT=public

# Optional additional variants for thumbnails/large previews
CF_IMAGES_VARIANT_THUMB=thumb
CF_IMAGES_VARIANT_LARGE=large
VITE_CF_IMAGES_VARIANT_THUMB=thumb
VITE_CF_IMAGES_VARIANT_LARGE=large

# AI (optional for image-to-text suggestions)
# Currently uses OpenAI; provide an API key to enable.
OPENAI_API_KEY=sk-...

# Stripe (optional for checkout handoff)
STRIPE_SECRET=sk_live_...
STRIPE_WEBHOOK_SECRET=whsec_...
SITE_URL=https://your-site.com
# Optional: default Stripe price for demo purchase button on /checkout
VITE_STRIPE_PRICE_ID=price_xxx
