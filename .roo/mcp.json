{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": ""
      }
    },
    "cloudflare": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://bindings.mcp.cloudflare.com/sse"
      ]
    }
  }
}