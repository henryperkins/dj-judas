{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": ""
      },
      "alwaysAllow": [
        "resolve-library-id"
      ]
    },
    "cloudflare-bindings": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://bindings.mcp.cloudflare.com/mcp"
      ]
    },
    "cloudflare-builds": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://builds.mcp.cloudflare.com/mcp"
      ]
    },
    "microsoft-learn": {
      "type": "streamable-http",
      "url": "https://learn.microsoft.com/api/mcp"
    }
  }
}