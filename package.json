    "tailwind-merge": "^3.3.1",
    "tslib": "^2.8.1",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.12.3",
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "pa11y": "^9.0.0",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^6.3.6",
    "wait-on": "^8.0.4",
    "wrangler": "^4.41.0"
  },
  "scripts": {
    "build": "tsc -b && vite build",
    "build:client": "vite build --mode production --outDir dist/client",
    "cf-typegen": "wrangler types",
    "check": "tsc && vite build && wrangler deploy --dry-run && npm run check:a11y",
    "deploy": "wrangler deploy",
    "deploy:netlify": "netlify deploy --prod",
    "dev": "vite",
    "lint": "eslint .",
    "preview": "npm run build && vite preview",
    "check:a11y": "sh -c 'vite preview --port 4173 --strictPort & PREVIEW_PID=$!; npx wait-on http://localhost:4173 && npx pa11y -c pa11y.json http://localhost:4173 --reporter cli --threshold 10; code=$?; kill $PREVIEW_PID; exit $code'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report"
  },
  "type": "module"
}
