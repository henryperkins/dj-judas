{
  "rules": [
    {
      "allowedOrigins": [
        "https://thevoicesofjudah.com",
        "https://www.thevoicesofjudah.com",
        "http://localhost:5173",
        "http://localhost:8787"
      ],
      "allowedMethods": ["GET", "PUT", "POST", "DELETE", "HEAD"],
      "allowedHeaders": [
        "Content-Type",
        "Content-Length",
        "Range",
        "If-Match",
        "If-None-Match",
        "Cache-Control",
        "X-Upload-Token"
      ],
      "exposeHeaders": [
        "ETag",
        "Content-Length",
        "Content-Range",
        "Content-Type"
      ],
      "maxAgeSeconds": 3600
    }
  ]
}
